{
  "name": "rytmi-api",
  "version": "3.6.3",
  "description": "Rytmi management system API",
  "repository": {
    "type": "git",
    "url": "https://github.com/codento/rytmi-api"
  },
  "main": "api/index.js",
  "scripts": {
    "start": "nodemon -w src --exec \"babel-node src/api\"",
    "debug": "nodemon -w src --inspect --exec \"babel-node src/api\"",
    "build": "babel src -d dist",
    "serve": "sequelize db:migrate && sequelize db:seed:all && node dist/api",
    "docker-build": "docker-compose build",
    "test": "jest --forceExit --runInBand",
    "testwatch": "jest --watch --forceExit",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "create-html-doc": "node node_modules/spectacle-docs/bin/spectacle.js -t doc/ doc/openapi-spec.json",
    "containers": "docker-compose up -d && echo Rytmi API is now up and running.",
    "stop": "docker-compose down",
    "migrate": "sequelize db:migrate && sequelize db:seed:all",
    "lint": "eslint **/*.js",
    "reset-dev-db": "docker-compose down -v && docker-compose up -d postgres"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "globalSetup": "./test/testSetup.js",
    "globalTeardown": "./test/testTeardown.js",
    "coverageReporters": [
      "lcov",
      "json",
      "html",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!.eslintrc.js",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/test/**",
      "!**/coverage/**",
      "!**/db/migrations/**",
      "!**/db/seeders_development/**"
    ]
  },
  "author": "Codento Oy",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "body-parser": "^1.19.0",
    "canvas": "^2.5.0",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-winston": "^2.6.0",
    "google-auth-library": "^1.6.1",
    "googleapis": "^39.2.0",
    "jsdom": "^15.1.1",
    "jsonwebtoken": "^8.5.1",
    "node-cron": "^2.0.3",
    "pg": "^7.11.0",
    "sequelize": "^4.44.2",
    "sequelize-json-schema": "^1.3.0",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.7",
    "validator": "^10.11.0",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "faker": "^4.1.0",
    "husky": "^1.2.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "rosie": "^1.6.0",
    "sequelize-cli": "^5.5.0",
    "superagent": "^3.8.3",
    "superagent-defaults": "^0.1.14",
    "supertest": "^3.4.2",
    "umzug": "^2.2.0"
  }
}
